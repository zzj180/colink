<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- xiaodong 20150610 modify .NoTitleBar to NoTitleBar.Fullscreen for Fullscreen FLAG_LAYOUT_IN_SCREEN -->
    <style name="AppTheme" parent="android:Theme.Light.NoTitleBar.Fullscreen">
        <item name="android:colorBackgroundCacheHint">@null</item>
        <item name="android:scrollbarThumbHorizontal">@android:color/transparent</item>
        <item name="android:scrollbarThumbVertical">@drawable/scrollbar_thumb_vertical_bg</item>
        <item name="android:scrollbarSize">3dp</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowNoTitle">true</item>
    </style>

    <!-- xiaodong 20150616 modified progress_bar_drawable to -->
    <style name="ProgressBarHorizontalStyle" parent="android:style/Widget.ProgressBar">
        <item name="android:indeterminateOnly">false</item>
        <item name="android:indeterminateDrawable">@android:drawable/progress_indeterminate_horizontal</item>
        <item name="android:progressDrawable">@drawable/progress_bar_drawable</item>
    </style>

    <style name="ProgressBarWebViewLoadingStyle" parent="android:style/Widget.ProgressBar">
        <item name="android:indeterminateOnly">false</item>
        <item name="android:indeterminateDrawable">@android:drawable/progress_indeterminate_horizontal</item>
        <item name="android:progressDrawable">@drawable/webview_progress_bar</item>
    </style>
    
    <style name="TextEditorDialog" parent="android:Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@null</item>
        <item name="android:windowIsFloating">false</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>

    <style name="PopupAnimation" mce_bogus="1" parent="android:Animation">
        <item name="android:windowEnterAnimation">@anim/popup_enter</item>
        <item name="android:windowExitAnimation">@anim/popup_exit</item>
    </style>

    <style name="GuideDialog">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowBackground">@color/black</item>
    </style>

    <style name="PoiRatingBar" parent="@android:style/Widget.RatingBar">
        <item name="android:progressDrawable">@drawable/rating_bar_poi_full_filled</item>
        <item name="android:minHeight">11dip</item>
        <item name="android:maxHeight">11dip</item>
    </style>

    <!-- 2013.02.23 added by shichao for sms dialog -->
    <style name="Theme.SmsNewCustomDialog" parent="android:Theme.Light">
        <item name="android:windowBackground">@color/sms_title</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowFrame">@null</item>
    </style>
    <!--  -->

    <style name="PauseDialogAnimation">
        <item name="android:windowEnterAnimation">@android:anim/slide_in_left</item>
        <item name="android:windowExitAnimation">@android:anim/slide_out_right</item>
    </style>

    <!-- 使用帮助字体样式 -->
    <style name="text_body">
        <item name="android:textSize">@dimen/help_submenu_text_size</item>
        <item name="android:textColor">@color/grey_white</item>
